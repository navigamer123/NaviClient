  "description": "A mod that draws beacon beams on top of every axolotl in render distance.",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/axolotlv1/icon.png",
  "environment": "client",
  "entrypoints": {
