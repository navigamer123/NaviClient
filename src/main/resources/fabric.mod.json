      "org.navigamer.axolotlv1.client.Axolotlv1DataGenerator"
    ],
    "client": [
      "org.navigamer.axolotlv1.client.Axolotlv1Client"
    ],
    "main": [
      "org.navigamer.axolotlv1.Axolotlv1"
    ],
    "modmenu": [
      "org.navigamer.axolotlv1.client.GUIConfigModMenu"
    ]
  },
  "mixins": [
